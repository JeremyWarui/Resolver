# API Configuration
# Vite automatically sets MODE based on command:
# - 'npm run dev' → MODE = 'development'
# - 'npm run build' → MODE = 'production'
# No need to manually set VITE_ENV

# Development API (local Django server)
VITE_API_URL_DEV=http://localhost:8000/api

# Production API (Neon database on Render)
VITE_API_URL_PROD=https://django-resolver.onrender.com/api
